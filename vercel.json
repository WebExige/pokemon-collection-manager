{
  "version": 2,
  "name": "poke-col-awouki",
  "functions": {
    "api/pokemon/[...path].js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/pokemon/(.*)",
      "destination": "/api/pokemon/[...path].js"
    }
  ],
  "headers": [
    {
      "source": "/api/pokemon/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-Api-Key"
        }
      ]
    }
  ]
}
# Configuration Firebase
# Obtenez ces valeurs depuis votre console Firebase : https://console.firebase.google.com/
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id

# Configuration API PokéTCG (optionnel mais recommandé)
# Obtenez une clé API gratuite sur : https://pokemontcg.io/
VITE_POKEMON_API_KEY=your_pokemon_api_key_here

# Configuration de l'environnement
VITE_APP_TITLE=Pokémon Collection Manager
VITE_APP_DESCRIPTION=Gestionnaire de collection de cartes Pokémon TCG
VITE_APP_URL=https://votredomaine.fr

# Configuration pour le mode production
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PERFORMANCE_MONITORING=false